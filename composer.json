{
    "name": "s360digital/laravel-linter",
    "description": "Linting for your laravel project",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "tightenco/tlint": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "S360Digital\\LaravelLinter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "sempro/phpunit-pretty-print": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0"
    }
}

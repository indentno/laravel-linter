{
    "name": "s360digital/laravel-linter",
    "description": "Linting for your laravel project",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "tightenco/tlint": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "S360Digital\\LaravelLinter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^2.18",
        "symfony/var-dumper": "^5.2"
    }
}
